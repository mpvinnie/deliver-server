{
  "compilerOptions": {
    "target": "es2016",
    "sourceMap": true,
    "lib": [
      "esnext"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true
  }
}